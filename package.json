{
    "name": "cookcookcook",
    "version": "1.0.0",
    "description": "Application de gestion de recettes de cuisine",
    "main": "backend-starter.js",
    "scripts": {
        "frontend:install": "cd frontend && npm install",
        "frontend:dev": "cd frontend && npm run dev",
        "frontend:build": "cd frontend && npm run build",
        "frontend:preview": "cd frontend && npm run preview",
        "backend:dev": "cd backend && deno task dev",
        "backend:run": "cd backend && deno task run",
        "dev": "npm-run-all --parallel backend:dev frontend:dev",
        "install-all": "npm run frontend:install",
        "build": "npm run frontend:build",
        "start": "node backend-starter.js",
        "render-start": "node backend-starter.js"
    },
    "keywords": [
        "recipe",
        "cooking",
        "sveltekit",
        "deno"
    ],
    "author": "Matteo, Valentin, Enzo",
    "license": "MIT",
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
